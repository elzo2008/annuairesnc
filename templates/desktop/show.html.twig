{% extends 'base.html.twig' %}

{% block title %}Desktop{% endblock %}

{% block content %}
    <h1>Desktop</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ desktop.id }}</td>
            </tr>
            <tr>
                <th>NomOrdinateur</th>
                <td>{{ desktop.nomOrdinateur }}</td>
            </tr>
            <tr>
                <th>Proprietaire</th>
                <td>{{ desktop.proprietaire }}</td>
            </tr>
            <tr>
